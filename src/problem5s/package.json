    "_moduleAliases": {
        "@controllers": "dist/controllers",
        "@models": "dist/models",
        "@services": "dist/services",
        "@config": "dist/config",
        "@exceptions": "dist/exceptions",
        "@midlewares": "dist/midlewares",
        "@swagger": "dist/swagger",
        "~": "dist",
        "@util": "dist/util",
        "@views": "dist/views"
    }

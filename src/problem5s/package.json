        "ejs": "^3.1.8",
        "errorhandler": "1.5.1",
        "express": "4.17.1",
        "express-flash": "0.0.2",
        "express-session": "1.17.1",
        "fbgraph": "1.4.4",
        "http-status": "^1.5.0",
        "joi": "^17.4.0",
        "lodash": "^4.17.20",
        "module-alias": "^2.2.2",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.43",
        "mongoose": "5.11.15",
        "mongoose-keywords": "^0.4.1",
        "mongoose-slugger-plugin": "^3.0.2",
        "querymen": "^2.1.4",
        "rxjs": "^7.8.1",

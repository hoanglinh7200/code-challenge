    "scripts": {
        "build-ts": "tsc",
        "build": "npm run build-ts && npm run lint",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "dev": "node dist/server.js",
        "start": "concurrently \"npm run watch-ts\" \"npm run watch-node\"",
